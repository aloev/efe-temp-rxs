: [],\n    loading: false,\n    error: null\n}\n\nconst buildNewCategories = ( papaId, categories, category ) => {\n    \n    let myCategories = [];\n\n    // if ain't no son\n\n    if( papaId == undefined ){\n\n        return [\n            ...categories,\n            {\n                _id: category._id,\n                name: category.name,\n                slug: category.slug,\n                children: [],\n            }\n\n        ];\n    }\n\n\n    // if they  == , we know is a son category\n\n    for(let cat of categories ){\n\n        //if papaID given equals id within categories arrays passed\n\n        if(cat._id == papaId ){\n            // Tree expansion at its finest\n    \n            myCategories.push({\n                ...cat,\n                // You just wanna modify the category, so that's the only one u have to pass\n                children: cat.children ? buildNewCategories(papaId, [...cat.children, {\n                    _id: category._id,\n                    name: category.name,\n                    slug: category.slug,\n                    parentId: category.parentId,\n                    children: category.children,\n                }], category) : []\n            });\n        } else {\n            // If they are diff, \n            myCategories.push({\n                ...cat,\n                children: cat.children ? buildNewCategories(papaId, cat.children, category) : []\n            });\n        }\n\n    }\n\n    return myCategories;\n}\n\nexport const categoryReducer = ( state = initialState, action ) => {\n\n    switch(action.type){\n\n        case categoryConstants.GET_ALL_CATEGORIES_SUCCESS:\n            state = {\n                ...state,\n                categories: action.payload.categories\n            }\n            break;\n\n        case categoryConstants.ADD_NEW_CATEGORY_REQUEST:\n            state = {\n                ...state,\n                loading: true\n            }\n            break;\n        \n        case categoryConstants.ADD_NEW_CATEGORY_SUCCESS:\n            \n            const category =  action.payload.category;\n            const updatecat = buildNewCategories( category.parentId ,state.categories, category);\n\n            state = {\n                ...state,\n                loading: false,\n                categories: updatecat,\n            }\n            break;\n        \n        case categoryConstants.ADD_NEW_CATEGORY_FAILURE:\n            state = {\n                ...initialState\n            }\n            break;\n    }\n\n    return state;\n}","/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/urlConfig.js",[],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/store/index.js",[],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/components/Layout/index.js",[],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/containers/Products/index.js",[],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/components/UI/Modal/index.js",[],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/containers/Home/index.js",["142","143","144","145","146"],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/actions/auth_actions.js",["147"],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/components/UI/Input/index.js",[],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/containers/Signup/index.js",["148","149"],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/index.js",[],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/reducers/user_reducer.js",["150","151"],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/actions/initialD_actions.js",["152","153"],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/actions/constants.js",[],"/Users/miguelangel/Desktop/React/React-Ecommerce/temp/admin-app/src/reducers/index.js",["154"],{"ruleId":"155","severity":1,"